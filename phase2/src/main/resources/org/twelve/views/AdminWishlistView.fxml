<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="graphic" hgap="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" vgap="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.twelve.views.AdminWishlistView">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <VBox prefHeight="200.0" prefWidth="100.0">
            <children>
                <Label text="%accounts">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin></Label>
                <ListView fx:id="allUsers" prefWidth="200.0" />
            </children>
        </VBox>
        <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1">
            <children>
                <Label text="%wishlist" />
                <ListView fx:id="wishlistOfUser" prefWidth="200.0" />
            </children>
        </VBox>

        <Label fx:id="itemDescription" maxWidth="1.7976931348623157E308" text="%itemDescription" wrapText="true" GridPane.columnSpan="2" GridPane.rowIndex="1" />
        <HBox alignment="BASELINE_RIGHT" prefHeight="6.0" prefWidth="285.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
            <children>
                <Button fx:id="removeButton" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#removeClicked" text="%remove" />
            </children></HBox>
        <Button mnemonicParsing="false" onAction="#backClicked" text="%back" GridPane.rowIndex="2" />

    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</GridPane>
